<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title><%=title %></title>
  <link rel="stylesheet" href="/stylesheets/users.css">
  <link rel="icon" href="/images/favicon.ico">
</head>
<body>
  <header>
    <div class="container">
      <h1><%=title %></h1>
    </div>
  </header>

  <section class="link">
    <div>
      <div>
        <div><a href=<%= index %>>back to the top page</a></div>
      </div>
      <div>
        <div><a href=<%= books %>>all books</a></div>
      </div>
    </div>
  </section>

  <section class="output">
    <div>
      <p>all users</p>
      <table>
        <tr class="column">
          <td>id</td>
          <td>company</td>
          <td>name</td>
          <td>occupation</td>
        </tr>
        <% for (let i = 0; i < content.length; i++) { %>
          <tr>
            <td><%= content[i].id %></td>
            <td><%= content[i].company %></td>
            <td><%= content[i].name %></td>
            <td><%= content[i].occupation %></td>
          </tr>
        <% } %>
      </table>
    </div>
  </section>

  <footer>
    <div>
      <p><%= libraryName %></p>
    </div>
  </footer>

</body>
</html>
